{
  "name": "interest-rate-simulator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier-format": "run-script-os",
    "prettier-format:win32": "prettier --config .prettierrc \"./pages/**/*.(tsx|ts)\" --write",
    "prettier-format:darwin:linux": "prettier --config .prettierrc 'pages/**/*.(tsx|ts)' --write",
    "prettier-format:default": "prettier --config .prettierrc 'pages/**/*.(tsx|ts)' --write",
    "prettier-watch": "run-script-os",
    "prettier-watch:win32": "onchange \"pages/**/*.(tsx|ts)\" -- prettier --write {{changed}}",
    "prettier-watch:darwin:linux": "onchange 'pages/**/*.(tsx|ts)' -- prettier --write {{changed}}",
    "prettier-watch:default": "onchange 'pages/**/*.(tsx|ts)' -- prettier --write {{changed}}"
  },
  "dependencies": {
    "@types/node": "18.11.11",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.29.0",
    "eslint-config-next": "13.0.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "next": "13.0.6",
    "onchange": "^7.1.0",
    "prettier": "^2.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recharts": "^2.2.0",
    "run-script-os": "^1.1.6",
    "ts-node": "^10.9.1",
    "typescript": "4.9.3"
  }
